#Packages.R

if(!require(RODBC)){ 
  install.packages("RODBC")
  library(RODBC)
}

if(!require(readxl)){
  install.packages("readxl")
  library(readxl)
}

if(!require(dplyr)){
  install.packages("dplyr")
  library(dplyr)
}

